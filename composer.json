{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=7.4",
        "composer/installers": "^1.7",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/addtoany": "^1.10",
        "drupal/admin_toolbar": "^2.5",
        "drupal/better_exposed_filters": "^3.0",
        "drupal/block_class": "^1.0",
        "drupal/bootstrap": "^3.17",
        "drupal/color_field": "^2.0",
        "drupal/commerce": "^2.10",
        "drupal/commerce_bulk": "^1.0@alpha",
        "drupal/commerce_shipping": "^2.0@beta",
        "drupal/console": "^1.0.2",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-recommended": "^8.9",
        "drupal/devel": "^4.1",
        "drupal/entity_print": "2.x",
        "drupal/google_analytics": "^3.0",
        "drupal/international_phone": "^1.0",
        "drupal/linked_field": "^1.1",
        "drupal/mailchimp": "^1.8",
        "drupal/mailgun": "^1.0@beta",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/openapi": "^2.0",
        "drupal/openapi_jsonapi": "^3.0",
        "drupal/openapi_rest": "^2.0@RC",
        "drupal/openapi_ui": "^1.0@RC",
        "drupal/openapi_ui_redoc": "^1.0@RC",
        "drupal/paragraphs": "^1.5",
        "drupal/pathauto": "^1.3",
        "drupal/rabbit_hole": "1.x-dev",
        "drupal/schemata": "^1.0@beta",
        "drupal/sms": "^2.0",
        "drupal/sms_clickatell": "^2.0",
        "drupal/twig_tweak": "^2.1",
        "drupal/views_infinite_scroll": "^1.5",
        "drupal/viewsreference": "^1.4",
        "drupal/weight": "^3.1",
        "drupal/yoast_seo": "^1.5",
        "drush/drush": "^9.0.0",
        "tecnickcom/tcpdf": "~6",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "drupal/core-dev": "^8.9",
        "zaporylie/composer-drupal-optimizations": "^1.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "platform": {"php": "7.4"}
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/international_phone": {
                "External reference to jQuery 1.11.1 is not required": "https://www.drupal.org/files/issues/2018-07-12/international_phone-remove_external_jquery_ref-2985243-1-D8.patch",
                "Module doesn't work with jQuery 3+": "https://www.drupal.org/files/issues/fix-type-error-in-international-phone_2.patch",
                "Fix field label": "https://www.drupal.org/files/issues/2018-07-12/international_phone-fix_custom_field_label-2915431-10-D8.patch"
            },
            "drupal/commerce": {},
            "drupal/commerce_shipping": {
                "Support taxing the shipping cost": "https://www.drupal.org/files/issues/2018-10-03/shipping-tax-2874158-20.patch"
            },
            "drupal/rabbit_hole": {
                "Support for the commerce entities": "https://www.drupal.org/files/issues/commerce_support-2900548-3.patch"
            },
            "drupal/core": {
                "ResourceTypeRepository wrongly assumes that all entity reference fields have the setting \"target_type\"": "https://www.drupal.org/files/issues/2019-11-29/3057545-27.patch"
            },
            "drupal/dynamic_entity_reference": {
                "Incompatibility with jsonapi relatable resource search": "https://www.drupal.org/files/issues/2019-12-21/3057556-4.patch"
            },
            "drupal/openapi_jsonapi": {
                "Issue #3185778 TypeError: Cannot read property 'anyOf' of undefined": "https://www.drupal.org/files/issues/2020-12-21/add_required_keyword_8.x_2.x-3185778-5.patch",
                "Issue #3079209 Only show enabled endpoints for JSON:API": "https://www.drupal.org/files/issues/2020-12-21/only_add_read_route-methods_when_read_only_is_enabled_in_jsonapi_8.x_2.x-3079209-11.patch"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        }
    }
}
